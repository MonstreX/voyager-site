    "autoload-dev": {
        "psr-4": {
            "MonstreX\\VoyagerSite\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MonstreX\\VoyagerSite\\VoyagerSiteServiceProvider"
            ]
        }
    }
}
