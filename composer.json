            "name": "Yuriy Ovdeyev",
            "email": "fido6080net@gmail.com",
            "homepage": "http://ss-dev.ru"
        }
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/monstrex/voyager-site",
    "keywords": ["Laravel", "Voyager"],
    "require": {
        "monstrex/voyager-extension": "^0.81",
        "liquid/liquid": "^1.4",
        "webwizo/laravel-shortcodes": "1.0.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "laravel/framework": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MonstreX\\VoyagerSite\\": "src/"
